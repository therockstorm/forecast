{
  "name": "forecast",
  "version": "1.0.0",
  "scripts": {
    "build": "sls webpack",
    "build:stack": "cd scripts && tsc && cdk synth my-stack --app 'node stack.js' --no-version-reporting --strict > stack.yml",
    "clean": "skripts src scripts --pattern '*.js'",
    "deploy": "npm test && npm run build:stack && sls deploy",
    "format": "skripts prettier",
    "invoke": "STAGE=local sls invoke local --function func",
    "lint": "skripts tslint",
    "jest": "STAGE=test skripts jest",
    "test": "npm run lint && npm run jest && sls package",
    "watch": "npm run jest -- --watch"
  },
  "dependencies": {
    "@therockstorm/utils": "^3.0.11",
    "axios": "^0.19.0",
    "source-map-support": "^0.5.13"
  },
  "devDependencies": {
    "@aws-cdk/aws-codebuild": "^1.8.0",
    "@aws-cdk/core": "^1.8.0",
    "aws-sdk": "^2.528.0",
    "husky": "^3.0.5",
    "serverless-plugin-aws-alerts": "^1.4.0",
    "serverless-plugin-canary-deployments": "^0.4.8",
    "serverless-pseudo-parameters": "^2.4.0",
    "skripts": "0.0.23",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "skripts lint-staged"
    }
  }
}
